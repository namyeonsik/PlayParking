<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="flying.paypoint">

 <select id="selectBymid" 
  	parameterType="String"
  	resultType="com.flying.model.PayPointDTO">
    select * from paypoint where mid = #{aa:VARCHAR} order by paydate
  </select>
 
  <insert id="insertPoint" parameterType="com.flying.model.PayPointDTO">
	insert into PayPoint(paydate,mid,paypoint)
	values(sysdate,#{mid:VARCHAR},
	       #{paypoint:INTEGER})
  </insert>
  
</mapper>